{
    "version": 2,
    "builds": [
        {
            "src": "reviewproject/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/media_image/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "reviewproject/wsgi.py"
        }
    ],
    "functions": {
        "reviewproject/wsgi.py": {
            "maxDuration": 30
        }
    }
}