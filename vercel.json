{
    "version": 2,
    "builds": [
        {
            "src": "reviewproject/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        },
        {
            "src": "staticfiles",
            "use": "@vercel/static"
        },
        {
            "src": "media",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "staticfiles/$1"
        },
        {
            "src": "/media_image/(.*)",
            "dest": "media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "reviewproject/wsgi.py"
        }
    ]
}